{
    "version": "2.1.0",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "outDir": "dist/",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "lib": ["dom", "es5", "scripthost", "es2015"]
    },
    "files": [
        "./src/StatsdReporter.ts",
        "./src/StatusPageReporter.ts",
        "./src/Registry.ts",
        "./src/index.ts",
        "./src/metrics.ts",
        "./src/test/metrics_test.ts"
    ]
}
